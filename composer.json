{
  "name": "tourze/workerman-block-protocol",
  "description": "Workerman积木协议",
  "license": "MIT",
  "require": {
    "ext-json": "*",
    "ext-zlib": "*",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\BlockProtocol\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\BlockProtocol\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse --configuration=phpstan.neon",
    "test": "phpunit"
  }
}