{
  "name": "tourze/workerman-block-protocol",
  "description": "Workerman积木协议",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-zlib": "*",
    "workerman/workerman": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\BlockProtocol\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\BlockProtocol\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}